{
  "name": "ramlev/pakkelabels",
  "type": "library",
  "description": "PHP-SDK to communicate with Pakkelabels.dk",
  "homepage": "http://github.com/ramlev/pakkelabels-dk",
  "license": "MIT",
  "authors": [
    {
      "name": "Lars Olesen",
      "email": "lars@intraface.dk"
    },
    {
      "name": "Hasse Ramlev Hansen",
      "email": "hasse@ramlev.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "PakkelabelsDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
